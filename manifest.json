{
  "manifest_version": 2,
  "name": "ASCOM Dev Extension",
  "description": "Chrome Extension to Alaska developers",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "thirdParty/env.js",
      "thirdParty/jquery-3.2.1.min.js",
      "thirdParty/managetrip.js"
    ]
  },
  "web_accessible_resources": [
    "thirdParty/env.js",
    "thirdParty/jquery-3.2.1.min.js",
    "thirdParty/managetrip.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "thirdParty/env.js",
        "thirdParty/jquery-3.2.1.min.js",
        "thirdParty/managetrip.js"
      ]
    }
  ]
}