{
  "manifest_version": 2,
  "name": "ASCOM Dev Extension",
  "description": "Chrome Extension to Alaska developers",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "*://*.alaskaair.com/*",
    "tabs"
  ],
  "web_accessible_resources": [
    "logo.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.alaskaair.com/*"
      ],
      "js": [
        "thirdParty/jquery-3.2.1.min.js",
        "thirdParty/bootstrap-3.3.7-dist/js/bootstrap.min.js"
      ],
      "css": [
        "thirdParty/bootstrap-3.3.7-dist/css/bootstrap.min.css"
      ]
    }
  ]
}