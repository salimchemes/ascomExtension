{
  "manifest_version": 2,
  "name": "ASCOM Dev Extension",
  "description": "Chrome Extension to Alaska developers",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": ["logo.png"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
	  "js": ["thirdParty/jquery-3.2.1.min.js", "thirdParty/bootstrap.min.js", "thirdParty/popper.min.js"],
	  "css": ["css/bootstrap.min.css"]
    }
  ]
}